<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.competition">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/title"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/title_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Select"
            android:screenOrientation="portrait" />
        <activity android:name=".Contents"
            android:screenOrientation="portrait" />
        <activity android:name=".Choose"
            android:screenOrientation="portrait" />
        <activity android:name=".Insert"
            android:screenOrientation="portrait" />
        <activity android:name=".Find"
            android:screenOrientation="portrait" />
        <activity android:name=".Buildingcontents"
            android:screenOrientation="portrait" />
        <activity android:name=".Faq"
            android:screenOrientation="portrait" />
    </application>

</manifest>